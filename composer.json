{
    "name": "meanify-co/laravel-activity-log",
    "description": "A PHP library to generate logs from requests and model updates in Laravel",
    "authors": [
        {
            "name": "Daniel Barcelos",
            "email": "daniel@meanify.co"
        }
    ],
    "keywords": [
        "utils",
        "helpers",
        "activity",
        "log",
        "audit",
        "php",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "illuminate/support": "^10.0|^11.0",
        "symfony/yaml": "^6.0",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Meanify\\LaravelActivityLog\\": "src/",
            "Meanify\\LaravelActivityLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meanify\\LaravelActivityLog\\Providers\\MeanifyLaravelActivityLogServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
